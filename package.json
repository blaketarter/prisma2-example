{
  "name": "prisma2",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/server",
    "start:dev": "ts-node-dev --type-check --respawn --no-notify --  src/server",
    "test": "run-s build test:server",
    "test:ci": "CI=true npm run test",
    "test:server": "PORT=5000 start-server-and-test start 'http://localhost:5000' test:server:curl",
    "test:server:curl": "PORT=5000 curl 'http://localhost:5000/graphql' -H 'Content-Type: application/json' -H 'Accept: application/json' --data-binary '{\"query\":\"query { me { id } }\"}' --compressed -i --fail",
    "tsc": "tsc",
    "clean": "rm -rf dist",
    "build": "run-s clean generate tsc",
    "prisma:log": "ts-node prisma/log.ts",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio --experimental",
    "migration:create": "prisma migrate save --experimental",
    "migration:run": "run-s migration:run:up generate",
    "migration:run:up": "prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/schema.ts",
    "generate": "run-s generate:*",
    "postinstall": "npm run generate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.11.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/cli": "^2.2.2",
    "@prisma/client": "^2.2.2",
    "apollo-server": "^2.15.1",
    "apollo-server-express": "^2.15.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "graphql": "^14.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.2",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "nexus-prisma": "^0.14.0"
  },
  "engines": {
    "node": "10.x.x"
  }
}
