{
  "name": "prisma2",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/server",
    "start:dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "tsc": "tsc",
    "clean": "rm -rf dist",
    "build": "run-s clean generate tsc",
    "prisma:log": "ts-node prisma/log.ts",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio --experimental",
    "migration:create": "prisma migrate save --experimental",
    "migration:run": "run-s migration:run:up generate",
    "migration:run:up": "prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema.ts",
    "generate": "run-s generate:*",
    "postinstall": "npm run generate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.13.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.3",
    "apollo-server": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.6",
    "jsonwebtoken": "^8.5.1",
    "nexus-prisma": "^0.12.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
